Cell[CellGroupData[{
Cell["Test DAE for implicit Runge-Kutta integrators", "Title",
 CellChangeTimes->{{3.787808910945627*^9, 3.7878089255045147`*^9}}],

Cell[CellGroupData[{

Cell["DAE and the corresponding ODE", "Section",
 CellChangeTimes->{{3.787809239007183*^9, 3.787809239322794*^9}, {
  3.787809581449868*^9, 3.787809586465963*^9}}],

Cell["The following DAE is solved:", "Text",
 CellChangeTimes->{{3.787809247832419*^9, 3.7878092547609863`*^9}}],

Cell[BoxData[{
 FormBox[
  RowBox[{"0", "=", 
   RowBox[{
    RowBox[{
     FormBox[
      RowBox[{
       SubscriptBox["\[PartialD]", "t"], "x"}],
      TraditionalForm], 
     RowBox[{"(", "t", ")"}]}], "+", 
    RowBox[{"z", "(", "t", ")"}]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"0", "=", 
   RowBox[{
    RowBox[{"x", "(", "t", ")"}], "-", 
    SuperscriptBox[
     RowBox[{"z", "(", "t", ")"}], "2"]}]}], 
  TraditionalForm]}], "DisplayFormula",
 CellChangeTimes->{{3.787809266647346*^9, 3.787809284725996*^9}, {
  3.787809404246462*^9, 3.7878094580964613`*^9}, {3.7878094953941393`*^9, 
  3.787809495396327*^9}}],

Cell["which corresponds to the following ODE", "Text",
 CellChangeTimes->{{3.78780947730584*^9, 3.787809504443912*^9}, {
  3.787809544367206*^9, 3.787809551626507*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    FormBox[
     RowBox[{
      SubscriptBox["\[PartialD]", "t"], "x"}],
     TraditionalForm], 
    RowBox[{"(", "t", ")"}]}], "=", 
   RowBox[{"-", 
    SqrtBox[
     RowBox[{"x", "(", "t", ")"}]]}]}], TraditionalForm]], "DisplayFormula",
 CellChangeTimes->{{3.787809266647346*^9, 3.787809284725996*^9}, {
  3.787809404246462*^9, 3.7878094580964613`*^9}, {3.7878094953941393`*^9, 
  3.787809533804221*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ode", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"x", "'"}], "[", "t", "]"}], "\[Equal]", 
      RowBox[{"-", 
       SqrtBox[
        RowBox[{"x", "[", "t", "]"}]]}]}], ",", 
     RowBox[{
      RowBox[{"x", "[", "0", "]"}], "\[Equal]", "x0"}]}], "}"}]}], 
  ";"}]], "Input",
